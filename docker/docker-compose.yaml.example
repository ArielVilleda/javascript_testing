version: '3.7'

services:
  variable_commissions_front:
    container_name: react_project
    build:
      context: ../
      dockerfile: ./docker/Dockerfile
    volumes:
      - '../:/app'
      - '/app/node_modules'
    ports:
      - 3001:3000
    stdin_open: true
    environment:
      - CHOKIDAR_USEPOLLING=true
      - BROWSER=none
    command: ["npm", "start"]